    "gatsby-plugin-manifest": "^3.2.0",
    "gatsby-plugin-offline": "^4.2.0",
    "gatsby-plugin-react-helmet": "^4.2.0",
    "gatsby-plugin-sharp": "^3.2.1",
    "gatsby-plugin-styled-components": "^4.2.0",
    "gatsby-source-filesystem": "^3.2.0",
    "gatsby-transformer-remark": "^3.2.0",
    "gatsby-transformer-sharp": "^3.2.0",
    "intersection-observer": "^0.12.0",
    "polished": "^4.1.1",
    "postcss": "^8.2.9",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^8.28.6",
    "react-is": "^17.0.2",
    "react-spring": "^9.1.0",
    "rehype-react": "^6.0.0",
    "styled-components": "^5.2.3",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "prettier": "2.0.5"
  },
  "keywords": [
    "portfolio"
  ],
  "license": "BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format:all": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format": "prettier --write",
    "lint": "eslint",
    "lint:all": "eslint \"./src/**/*.{js,jsx}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": [
      "npm run lint"
    ],
    "*.{js,json,css,md}": [
      "npm run format"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kalleilves/kalleilves.github.io"
  },
  "bugs": {
